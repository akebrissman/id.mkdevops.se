{
  "name": "id.mkdevops.se",
  "version": "0.7.6",
  "author": "National Library of Sweden",
  "license": "Apache-2.0",
  "description": "Simple OAuth2 provider",
  "homepage": "https://github.com/vtepliuk/id.mkdevops.se#readme",
  "bugs": {
    "url": "https://github.com/vtepliuk/id.mkdevops.se/issues"
  },
  "scripts": {
    "build": "rm -f assets/.DS_Store && NODE_ENV=production ./node_modules/.bin/webpack --progress --colors -p",
    "start": "./node_modules/.bin/concurrently -n \"WEBPACK,FLASK\" -c \"bgBlue.bold,bgMagenta.bold\" \"npm run webpack-dev-server\" \"npm run flask-server\"",
    "webpack-dev-server": "NODE_ENV=debug ./node_modules/.bin/webpack-dev-server --port 2992 --hot --inline",
    "flask-server": "FLASK_APP=$PWD/autoapp.py FLASK_DEBUG=1 flask run",
    "lint": "eslint \"assets/js/*.js\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vtepliuk/id.mkdevops.se"
  },
  "keywords": [
    "kb",
    "libris",
    "bibdb",
    "oauth2"
  ],
  "engines": {
    "node": ">=7.6.0",
    "npm": ">=5.3.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "babel-preset-es2015": "^6.24.1",
    "concurrently": "^4.0.1",
    "css-loader": "^1.0.0",
    "eslint": "^5.6.1",
    "eslint-config-google": "^0.10.0",
    "eslint-plugin-import": "^2.14.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^2.0.0",
    "font-awesome-webpack": "0.0.5-beta.2",
    "kungbib-styles": "^1.2.4",
    "less": "^3.8.1",
    "less-loader": "^4.1.0",
    "manifest-revision-webpack-plugin": "^0.4.1",
    "raw-loader": "^0.5.1",
    "style-loader": "^0.23.1",
    "url-loader": "^1.1.2",
    "webpack": "^4.20.2",
    "webpack-dev-server": "^3.1.9"
  },
  "dependencies": {
    "@babel/core": "^7.1.2",
    "ajv": "^6.5.4",
    "bootstrap": "^4.1.3",
    "font-awesome": "^4.7.0",
    "jquery": "^3.3.1",
    "popper.js": "^1.14.4"
  }
}
